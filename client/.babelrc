{
  "presets": [
      ["@babel/preset-env", {
          "targets": "> 0.25%, not dead",
          "modules": false,
          "useBuiltIns": "usage"
      }],
      "@babel/preset-react"
  ],
  "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties"
  ]
}