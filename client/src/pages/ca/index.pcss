.ca-wrapper{
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  align-items: center;
  justify-content: center;

  height: 100vh;
  transform: translate(0, -80px);
  
  img{
    display: block;
    width: 220px;
    height: 220px;
    pointer-events: none;
    user-select: none;
    cursor: default;
  }

  li{
    list-style: disc;
  }
}