{
  "bin": {
    "pooy": "bin/pooy"
  },
  "name": "pooy",
  "version": "0.1.0",
  "description": "http proxy",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node bin/pooy",
    "start-electron": "sudo electron ./app.js",
    "dev": "node scripts/dev.js",
    "pack": "electron-packager . pooy --platform=darwin --arch=x64 --ignore=node_modules --ignore=docs --ignore=README.md",
    "client:dev": "NODE_ENV=development webpack-dev-server --config client/webpack/webpack.dev.js",
    "build": "rm -rf client/dist && NODE_ENV=production webpack --config client/webpack/webpack.prod.js",
    "analyze": "rm -rf client/dist && NODE_ENV=production webpack --analyze --config client/webpack/webpack.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^3.0.2",
    "compare-versions": "^3.5.1",
    "mime": "^2.4.4",
    "ora": "^4.0.4",
    "qrcode": "^1.4.1",
    "socket.io": "^2.2.0",
    "yaml": "^1.7.0",
    "pooy-core": "0.1.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.4.4",
    "core-js": "^3.6.5",
    "marked": "^4.0.10",
    "prettier": "1.18.2",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "whatwg-fetch": "^3.0.0",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "electron-packager": "^14.0.6",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^4.3.1",
    "worker-loader": "^2.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/better-than-better/pooy.git"
  },
  "keywords": [
    "http",
    "https",
    "ssl",
    "proxy"
  ],
